      <TextPage id="1134" key="c2415140-c4b5-4457-bbc0-a9033dfac887" parentID="1122" level="3" creatorID="0" sortOrder="1" createDate="2016-02-16T10:06:15" updateDate="2016-02-16T11:29:51" nodeName="Matthieu Houx" urlName="matthieu-houx" path="-1,1063,1122,1134" isDoc="" nodeType="1062" creatorName="Julien Bouchareine" writerName="Julien Bouchareine" writerID="0" template="1057" nodeTypeAlias="TextPage">
        <hideInNavigation>1</hideInNavigation>
        <content><![CDATA[{
  "name": "1 column layout",
  "sections": [
    {
      "grid": 12,
