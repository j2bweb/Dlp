      <TextPage id="1147" key="1d44c2f3-ba80-420f-9a8e-8f81eaeb628c" parentID="1122" level="3" creatorID="0" sortOrder="4" createDate="2016-02-16T10:12:14" updateDate="2016-02-16T11:30:03" nodeName="Jessica Haté" urlName="jessica-haté" path="-1,1063,1122,1147" isDoc="" nodeType="1062" creatorName="Julien Bouchareine" writerName="Julien Bouchareine" writerID="0" template="1057" nodeTypeAlias="TextPage">
        <hideInNavigation>1</hideInNavigation>
        <content><![CDATA[{
  "name": "1 column layout",
  "sections": [
    {
      "grid": 12,
      "rows": [
        {
          "name": "Article",
          "areas": [
            {
              "grid": 4,
              "controls": [
